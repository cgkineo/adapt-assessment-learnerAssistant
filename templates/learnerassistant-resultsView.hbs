<div class="learnerassistant-resultsView-inner block-inner">
	<div class="learnerassistant-banner">
		<a href="#">
			<span><h5>Guided Learning</h5></span>
		</a>
	</div>
	<div class="learnerassistant-resultsView-filter-container">
		<!--<div class="learnerassistant-resultsView-filter clearfix">
			<a href="#" data-filter="failed">
				<span>Failed</span>
			</a>
			<a href="#" data-filter="passed">
				<span>Passed</span>
			</a>
			<a href="#" data-filter="all">
				<span>All</span>
			</a>
		</div>-->
	</div>
	<div class="content-container">
		<div class="content">	
			<div id="status" class="table-threetooone">
				<div class="table-threetoone-inner">
					<div class="col-header"><span class="icon icon-flag"></span><span>Status</span></div>
					<div class="mobile-only cell"><div class="cell-inner">{{#if options._isReviewComplete}}<span class="icon icon-star"></span><span>Reviewed</span>{{else}}<span class="icon icon-star-hollow"></span><span>Flagged for review</span>{{/if}}</div></div>
					{{#with options}}
						{{#each banks }}
							<div class="not-mobile cell rowspan-0"><div class="cell-inner rowspan-inherit">{{#if _isReviewComplete}}<span class="icon icon-star"></span><span>Reviewed</span>{{else}}<span class="icon icon-star-hollow"></span><span>Flagged for review</span>{{/if}}</div></div>
						{{else}}
							<div class="not-mobile cell rowspan-0"><div class="cell-inner rowspan-inherit">-</div></div>
						{{/each}}
					{{/with}}
				</div>
			</div>
			<div id="question" class="table-threetooone">
				<div class="table-threetoone-inner">
					<div class="col-header"><span class="icon icon-question"></span><span>Bank</span></div>
					{{#with options}}
						{{#each banks }}
							<div class="cell rowspan-0"><div class="cell-inner rowspan-inherit">{{title}}</div></div>
						{{else}}
							<div class="cell rowspan-0"><div class="cell-inner rowspan-inherit">-</div></div>
						{{/each}}
					{{/with}}
				</div>
			</div>
			<div id="tutorialcontent" class="table-threetooone">
				<div class="table-threetoone-inner">
					<div class="col-header"><span class="icon icon-eye"></span><span>Tutorial content</span></div>
					{{#with options}}
						{{#each banks }}
							<div class="cell rowspan-0"><div class="cell-inner rowspan-inherit">{{_countUnreviewedAssociatedLearning}} left or alternatively you're at {{_countReviewedAssociatedLearning}}/{{_countTotalAssociatedLearning}}</div></div>
						{{else}}
							<div class="cell rowspan-0"><div class="cell-inner rowspan-inherit">-</div></div>
						{{/each}}
					{{/with}}
				</div>
			</div>
		</div>
	</div>
	<a href="#" class="learnerassistant-close icon icon-cross"></a>
</div>